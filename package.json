{
  "name": "shopfront",
  "version": "1.0.0",
  "main": "dist/bin/serve.js",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ignore-pattern \"dist/*\" --ignore-pattern \"gas/*\" --ignore-pattern eslint.config.js --ignore-pattern jest.config.js",
    "lint:fix": "pnpm lint --fix",
    "gs:push": "(cd gas || exit 1) && pnpm push",
    "watch": "tsx watch src/bin/serve.ts | pino-pretty",
    "bin:serve": "node --enable-source-maps dist/bin/serve.js",
    "bin:cron": "node --enable-source-maps dist/bin/cron.js",
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@fast-csv/format": "^5.0.0",
    "@fastify/autoload": "^5.10.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@sinclair/typebox": "^0.32.34",
    "@types/google-apps-script": "^1.0.83",
    "bottleneck": "^2.19.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "fastify-print-routes": "^3.2.0",
    "mongodb": "^6.8.0",
    "pino": "^9.2.0",
    "shopfront": "link:",
    "ts-app-env": "^1.5.0",
    "undici": "^6.19.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.9",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^9.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "nock": "14.0.0-beta.7",
    "nock-record": "^0.3.9",
    "pino-pretty": "^11.2.1",
    "ts-jest": "^29.1.5",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  }
